{
  "repository": {},
  "scripts": {
    "copy-normalize-css": "cp -r ./node_modules/normalize.css/normalize.css priv/static/normalize",
    "copy-font-awesome-data": "cp -r ./node_modules/font-awesome/css priv/static/font-awesome; cp -r ./node_modules/font-awesome/fonts priv/static/font-awesome",
    "copy-mapbox-data": "cp -r ./node_modules/mapbox-gl/dist/ priv/static/mapbox/",
    "build-assets": "cp -r web/static/assets/* priv/static",
    "watch-assets": "watch-run -i -p 'web/static/assets/**' npm run build-assets",
    "build-browserify": "./node_modules/.bin/browserify web/static/ts/main.ts -p [ tsify ] > priv/static/js/main.js",
    "watch-browserify": "watchify web/static/ts/main.ts -p [ tsify ] -o priv/static/js/main.js -dv",
    "build-webpack": "webpack",
    "watch-webpack": "webpack -w",
    "build-rollup": "rollup -c",
    "watch-rollup": "rollup -c -w",
    "build-css": "sass web/static/css/base.scss priv/static/css/base.css",
    "watch-css": "sass --watch web/static/css:priv/static/css",
    "build": "npm run mk-priv-dirs; npm run build-browserify & npm run build-css & npm run build-assets & npm run copy-mapbox-data & npm run copy-font-awesome-data & npm run copy-normalize-css",
    "watch": "npm run mk-priv-dirs; npm run watch-browserify & npm run watch-css & npm run watch-assets & npm run copy-mapbox-data & npm run copy-font-awesome-data & npm run copy-normalize-css",
    "test": "browserify -t babelify test/static/ts/*.js | tap",
    "test-build": "watchify -v -t babelify web/static/ts/test.js -o priv/static/ts/test.js",
    "test-serve": "live-server --entry-file=test.html",
    "mk-priv-dirs": "rm -rf priv/static && mkdir -p priv/static; mkdir -p priv/static/ts; mkdir -p priv/static/normalize; mkdir -p priv/static/font-awesome; mkdir -p priv/static/mapbox; mkdir -p priv/static/js; mkdir -p priv/static/css",
    "deploy": "npm run mk-priv-dirs; npm run build-browserify && npm run build-css && npm run build-assets && npm run copy-mapbox-data && npm run copy-font-awesome-data && npm run copy-normalize-css"
  },
  "dependencies": {
    "@cycle/dom": "^14.0.0",
    "@cycle/http": "^11.2.0",
    "@cycle/isolate": "^1.4.0",
    "@cycle/rxjs-run": "^3.0.3",
    "@cycle/storage": "3.0.0-rc3",
    "cycle-mapboxgl": "0.0.9",
    "cyclic-router": "^3.0.1",
    "deep-assign": "^2.0.0",
    "deep-equal": "^1.0.1",
    "font-awesome": "^4.7.0",
    "history": "^3.0.0",
    "immutable": "^3.8.1",
    "js-cookie": "^2.1.3",
    "moment": "^2.13.0",
    "normalize.css": "^5.0.0",
    "parse-address": "0.0.5",
    "phoenix": "file:../../deps/phoenix",
    "phoenix_html": "file:../../deps/phoenix_html",
    "query-string": "^4.2.2",
    "rxjs": "^5.0.0-rc.2",
    "validator": "^5.2.0",
    "xstream": "^7.0.0"
  },
  "devDependencies": {
    "@types/chai": "^3.4.34",
    "@types/deep-equal": "0.0.30",
    "@types/history": "^2.0.38",
    "@types/js-cookie": "^2.0.28",
    "@types/phoenix": "0.0.4",
    "@types/query-string": "^3.0.28",
    "@types/validator": "^5.7.33",
    "browserify": "^13.1.1",
    "compression-webpack-plugin": "^0.3.1",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "ts-loader": "^0.9.1",
    "tsify": "^2.0.2",
    "typescript": "^2.0.6",
    "watch-run": "^1.2.4",
    "webpack": "^1.13.2"
  }
}
